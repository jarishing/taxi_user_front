<?xml version='1.0' encoding='utf-8'?>
<widget id="cc.kenyip.ngrok.taxi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name> 打的 DA DI </name>
    <description>
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icons/mipmap-mdpi/ic_launcher.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="mdpi" src="icons/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="icons/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="icons/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="icons/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="icons/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-speechrecognition" spec="^1.2.0" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <engine name="android" spec="^7.1.4" />
</widget>
